    "aws-sdk": "^2.449.0",
    "cognito-express": "^2.0.14",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.1",
    "mongoose": "^5.5.5",
    "node-fetch": "^2.5.0",
    "request": "^2.88.0"
  }
